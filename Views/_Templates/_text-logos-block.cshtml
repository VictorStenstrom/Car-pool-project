<section ew-if-key="text-logos-block" class="p-standard display-flex align-items-center justify-center slide-from-left" data-inviewanimation card-category>
    <div class="content-standard text-block-xxl align-center">
        <img class="logo" ew-for-src="section-logo" />
        <h4 class="weight-superbold letter-spacing-xxxl" ew-for="heading"></h4>
        <p ew-if-key="desc" class="size-sm m-b-5" ew-for="desc"></p>
        <div ew-if-key="partner-logo" class="grid-row grid-row-sm-gap col-6 col-lgphone-2" ew-list="partner-logo">
            <div class="display-flex direction-column align-center m-auto">
                <img class="gallery" ew-for-src="partner-logo" />
            </div>
        </div>
        <div ew-if-key="media">
            <img class="block-media" ew-for-src="media" />
        </div>
        <div class="full-line m-t-5"></div>
    </div>
</section>